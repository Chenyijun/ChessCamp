<% unless @camps.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Camps registered in:</h5>
    <ul class="side-nav">
      <% @camps.each do |camp| %>
        <li><%= link_to "#{camp.name}, #{camp.start_date.strftime('%m/%d/%y')}", camp_path(camp) %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>Student is not registered for any camps</h5>
  </div>
<% end %>